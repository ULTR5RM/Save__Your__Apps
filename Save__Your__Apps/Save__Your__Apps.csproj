    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>58fc100d-571f-4ef9-85df-dd6ac3eadf9c</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.5" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.14.0" />
    <PackageReference Include="Npgsql" Version="6.0.4" />
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="6.0.4" />
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties dbconnect_1json__JsonSchema="https://json.schemastore.org/config.json" /></VisualStudio></ProjectExtensions>

</Project>
